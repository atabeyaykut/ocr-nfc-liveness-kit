<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mkt_approval_page_parent_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mkt_breadcrumb_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/guideline1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_percent="0.25"/>
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_percent="0.5"/>
            <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:id="@+id/guideline3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintGuide_percent="0.75"/>
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/logo_container_0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_major"
                app:cardBackgroundColor="@color/colorPrimary"
                app:cardCornerRadius="160dp"
                app:cardElevation="@dimen/plane_04"
                app:cardUseCompatPadding="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_driver_info"
                    android:padding="@dimen/spacing_medium"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    app:srcCompat="@drawable/mkt_breadcrumb_driver"/>
            </com.google.android.material.card.MaterialCardView>
            <View
                android:id="@+id/item_divider"
                android:background="@color/colorPrimary"
                android:layout_width="0dp"
                android:layout_height="4dp"
                android:layout_marginTop="@dimen/spacing_major"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/logo_container_1"
                app:layout_constraintStart_toEndOf="@+id/logo_container_0"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/logo_container_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_major"
                app:cardBackgroundColor="@color/colorPrimary"
                app:cardCornerRadius="160dp"
                app:cardElevation="@dimen/plane_04"
                app:cardUseCompatPadding="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline2"
                app:layout_constraintStart_toEndOf="@+id/guideline1"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_car_info"
                    android:padding="@dimen/spacing_medium"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    app:srcCompat="@drawable/mkt_breadcrumb_report"/>
            </com.google.android.material.card.MaterialCardView>
            <View
                android:id="@+id/item_divider_2"
                android:background="@color/colorPrimary"
                android:layout_width="0dp"
                android:layout_height="4dp"
                android:layout_marginTop="@dimen/spacing_major"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/logo_container_2"
                app:layout_constraintStart_toEndOf="@+id/logo_container_1"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/logo_container_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_major"
                app:cardBackgroundColor="@color/colorPrimary"
                app:cardCornerRadius="160dp"
                app:cardElevation="@dimen/plane_04"
                app:cardUseCompatPadding="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintStart_toEndOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_approve_info"
                    android:padding="@dimen/spacing_medium"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    app:srcCompat="@drawable/mkt_breadcrumb_qr"/>
            </com.google.android.material.card.MaterialCardView>
            <View
                android:id="@+id/item_divider_3"
                android:background="@color/colorPrimary"
                android:layout_width="0dp"
                android:layout_height="4dp"
                android:layout_marginTop="@dimen/spacing_major"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/logo_container_3"
                app:layout_constraintStart_toEndOf="@+id/logo_container_2"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/logo_container_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_major"
                app:cardBackgroundColor="@color/mkt_breadcrumb_passive"
                app:cardCornerRadius="160dp"
                app:cardElevation="@dimen/plane_04"
                app:cardUseCompatPadding="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/guideline3"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_final_info"
                    android:padding="@dimen/spacing_medium"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    app:srcCompat="@drawable/mkt_breadcrumb_final"/>
            </com.google.android.material.card.MaterialCardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <include
            android:id="@+id/no_connection_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_minor"
            android:layout_marginStart="@dimen/spacing_minor"
            android:layout_marginEnd="@dimen/spacing_minor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mkt_breadcrumb_container"
            layout="@layout/include_no_connection"/>
        <include
            android:id="@+id/system_error_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_minor"
            android:layout_marginStart="@dimen/spacing_minor"
            android:layout_marginEnd="@dimen/spacing_minor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mkt_breadcrumb_container"
            layout="@layout/include_system_error"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mkt_preview_page_content_container"
            android:paddingBottom="@dimen/spacing_xxxlarge"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mkt_breadcrumb_container">
            <androidx.appcompat.widget.AppCompatTextView
                android:textAppearance="?attr/textAppearanceHeadline6"
                android:textSize="16sp"
                android:textStyle="bold"
                android:ellipsize="end"
                android:id="@+id/txt_approval_main_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_major"
                android:text="@string/mkt_preview_page_title"
                android:lineSpacingExtra="2sp"
                android:fontFamily="@font/arimo_bold"
                android:layout_marginStart="@dimen/spacing_major"
                android:layout_marginEnd="@dimen/spacing_major"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textAppearance="?attr/textAppearanceSubtitle1"
                android:textSize="16sp"
                android:ellipsize="end"
                android:id="@+id/txt_approval_main_description"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_minor"
                android:text="@string/mkt_preview_page_description"
                android:fontFamily="@font/arimo"
                android:layout_marginStart="@dimen/spacing_major"
                android:layout_marginEnd="@dimen/spacing_major"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_approval_main_title"
                style="@style/Item_Subtitle_3"/>
            <com.google.android.material.card.MaterialCardView
                android:layout_gravity="center"
                android:id="@+id/mkt_preview_page_main_card"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/spacing_major"
                app:cardCornerRadius="@dimen/radius_large"
                app:cardElevation="@dimen/plane_04"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_approval_main_description">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textAppearance="?attr/textAppearanceSubtitle1"
                        android:id="@+id/mkt_preview_report_number_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/spacing_major"
                        android:text="@string/mkt_preview_page_card_title"
                        android:textAllCaps="true"
                        android:fontFamily="@font/arimo_bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_report_date"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        android:layout_marginStart="@dimen/spacing_major"
                        android:layout_marginEnd="@dimen/spacing_major"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/mkt_preview_report_number_title">
                        <androidx.constraintlayout.widget.Guideline
                            android:orientation="vertical"
                            android:id="@+id/guideline_vehicle_licence"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layout_constraintGuide_begin="144dp"/>
                        <TextView
                            android:textSize="14sp"
                            android:textStyle="normal"
                            android:textColor="#999999"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/mkt_preview_page_card_report_date"
                            android:fontFamily="@font/arimo"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/guideline_vehicle_licence"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textAppearance="?attr/textAppearanceBody2"
                            android:textStyle="bold"
                            android:gravity="end"
                            android:id="@+id/preview_txt_date"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/arimo_bold"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/guideline_vehicle_licence"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:background="@color/light_holo"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="@dimen/spacing_medium"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/preview_txt_date"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_plate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        android:layout_marginStart="@dimen/spacing_major"
                        android:layout_marginEnd="@dimen/spacing_major"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_report_date">
                        <androidx.constraintlayout.widget.Guideline
                            android:orientation="vertical"
                            android:id="@+id/guideline_plate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layout_constraintGuide_begin="144dp"/>
                        <TextView
                            android:textSize="14sp"
                            android:textStyle="normal"
                            android:textColor="#999999"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/mkt_preview_page_card_report_plate"
                            android:fontFamily="@font/arimo"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/guideline_plate"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textAppearance="?attr/textAppearanceBody2"
                            android:textStyle="bold"
                            android:gravity="end"
                            android:id="@+id/preview_txt_report_plate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/arimo_bold"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/guideline_plate"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_insurance"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_plate">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_preview_card_insurance_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_preview_page_card_insurance_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/container_mkt_insurance_list"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_medium"
                            android:layout_marginBottom="@dimen/spacing_major"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_preview_card_insurance_title">
                            <com.airbnb.epoxy.EpoxyRecyclerView
                                android:id="@+id/rcl_mkt_insurance_list_list"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_hit_point"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_insurance">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_preview_page_hit_point_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_preview_page_card_impact_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/img_hit_point"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleType="centerCrop"
                            android:layout_marginStart="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_preview_page_hit_point_title"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_photos"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_hit_point">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_policy_vehicle_licence_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_preview_page_card_photos_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/container_mkt_vehicle_licence_plate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_medium"
                            android:layout_marginBottom="@dimen/spacing_major"
                            android:layout_marginStart="@dimen/spacing_major"
                            android:layout_marginEnd="@dimen/spacing_major"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_policy_vehicle_licence_title">
                            <com.airbnb.epoxy.EpoxyRecyclerView
                                android:id="@+id/rcl_mkt_car_list"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_preview_page_location"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_photos">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_policy_insurance_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_location_location_card_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.appcompat.widget.AppCompatTextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:textSize="16sp"
                            android:ellipsize="end"
                            android:id="@+id/txt_location_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_minor"
                            android:fontFamily="@font/arimo"
                            android:layout_marginStart="@dimen/spacing_major"
                            android:layout_marginEnd="@dimen/spacing_major"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_policy_insurance_title"
                            style="@style/Item_Subtitle_3"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_description"
                        android:paddingBottom="@dimen/spacing_minor"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_preview_page_location">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_accident_information_description_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_location_explanation_card_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.appcompat.widget.AppCompatTextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:textSize="16sp"
                            android:ellipsize="end"
                            android:id="@+id/txt_scenario_text"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_minor"
                            android:fontFamily="@font/arimo"
                            android:layout_marginStart="@dimen/spacing_major"
                            android:layout_marginEnd="@dimen/spacing_major"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_accident_information_description_title"
                            style="@style/Item_Subtitle_3"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:paddingBottom="@dimen/spacing_minor"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_description">
                        <TextView
                            android:textAppearance="?attr/textAppearanceSubtitle1"
                            android:id="@+id/mkt_accident_information_scenario_title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/spacing_major"
                            android:text="@string/mkt_preview_page_card_scenario_title"
                            android:fontFamily="@font/arimo_bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/img_scenario_point"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/spacing_minor"
                            android:scaleType="centerCrop"
                            android:layout_marginStart="@dimen/spacing_major"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mkt_accident_information_scenario_title"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView
                android:layout_gravity="center"
                android:id="@+id/mkt_matched_report_info_card"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/spacing_major"
                app:cardCornerRadius="@dimen/radius_large"
                app:cardElevation="@dimen/plane_04"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mkt_preview_page_main_card">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textAppearance="?attr/textAppearanceSubtitle1"
                        android:textColor="@color/mkt_card_title"
                        android:id="@+id/mkt_matched_licence_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/spacing_major"
                        android:text="@string/mkt_matched_report_card_title"
                        android:textAllCaps="true"
                        android:fontFamily="@font/arimo_bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/container_mkt_matched_licence_plate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        android:layout_marginStart="@dimen/spacing_major"
                        android:layout_marginEnd="@dimen/spacing_major"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/mkt_matched_licence_title">
                        <androidx.constraintlayout.widget.Guideline
                            android:orientation="vertical"
                            android:id="@+id/guideline_identity_number"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layout_constraintGuide_begin="144dp"/>
                        <TextView
                            android:textSize="14sp"
                            android:textStyle="normal"
                            android:textColor="#999999"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/mkt_matched_report_card_identity"
                            android:fontFamily="@font/arimo"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/guideline_identity_number"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textAppearance="?attr/textAppearanceBody2"
                            android:textStyle="bold"
                            android:gravity="end"
                            android:id="@+id/preview_txt_identity_number"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/arimo_bold"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/guideline_identity_number"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:background="@color/light_holo"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="@dimen/spacing_medium"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/preview_txt_identity_number"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/preview_plate_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/spacing_medium"
                        android:layout_marginBottom="@dimen/spacing_medium"
                        android:layout_marginStart="@dimen/spacing_major"
                        android:layout_marginEnd="@dimen/spacing_major"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/container_mkt_matched_licence_plate">
                        <androidx.constraintlayout.widget.Guideline
                            android:orientation="vertical"
                            android:id="@+id/guideline_matched_plate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layout_constraintGuide_begin="144dp"/>
                        <TextView
                            android:textSize="14sp"
                            android:textStyle="normal"
                            android:textColor="#999999"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="@string/mkt_matched_report_card_plate"
                            android:fontFamily="@font/arimo"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/guideline_matched_plate"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textAppearance="?attr/textAppearanceBody2"
                            android:textStyle="bold"
                            android:gravity="end"
                            android:id="@+id/preview_txt_plate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/arimo_bold"
                            android:layout_marginEnd="@dimen/spacing_minor"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/guideline_matched_plate"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:background="@color/light_holo"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="@dimen/spacing_medium"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/preview_txt_plate"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView
                android:layout_gravity="center"
                android:id="@+id/mkt_matched_report_warning_card"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/spacing_major"
                app:cardCornerRadius="@dimen/radius_large"
                app:cardElevation="@dimen/plane_04"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mkt_matched_report_info_card">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/spacing_micro">
                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/img_warning_icon"
                        android:clickable="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/mkt_warning_icon_rounded"
                        android:scaleType="centerCrop"
                        android:layout_marginStart="@dimen/spacing_minor"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textAppearance="?attr/textAppearanceSubtitle1"
                        android:textColor="@color/mkt_card_title"
                        android:id="@+id/mkt_warning_content"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/spacing_major"
                        android:text="@string/mkt_preview_page_card_warning_legal"
                        android:fontFamily="@font/arimo"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/img_warning_icon"
                        app:layout_constraintTop_toTopOf="parent"
                        style="@style/Item_Subtitle_3"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.button.MaterialButton
                android:id="@+id/btn_manual_next"
                android:padding="@dimen/spacing_major"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/spacing_major"
                android:text="@string/mkt_page_approve_button"
                android:textAllCaps="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mkt_matched_report_warning_card"
                style="@style/buttonWhite"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
